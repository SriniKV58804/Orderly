{
  "expo": {
    "scheme": "acme",
    "plugins": [
      "expo-router"
    ],
    "name": "IntelliPlanAI",
    "slug": "IntelliPlanAI",
    "experiments": {
      "newArchEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "proxy": {
        "/api": {
          "changeOrigin": true,
          "secure": true,
          "pathRewrite": {
            "^/api": ""
          }
        }
      }
    }
  }
}
